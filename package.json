{
  "name": "@allanchain/gridsome-plugin-pwa",
  "version": "0.4.0-alpha.5",
  "description": "Yet another PWA plugin for Gridsome",
  "main": "gridsome.server.js",
  "author": "AllanChain",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AllanChain/gridsome-plugin-pwa"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint --fix . --ext .js",
    "test": "jest --runInBand",
    "test:build": "jest __tests__/*.build.e2e.js --runInBand --detectOpenHandles",
    "test:develop": "jest __tests__/*.develop.e2e.js --runInBand --detectOpenHandles",
    "test:unit": "jest __tests__/*.unit.js"
  },
  "keywords": [
    "gridsome-plugin"
  ],
  "files": [
    "gridsome.*.js",
    "lib/*.js"
  ],
  "workspaces": [
    ".",
    "examples/*"
  ],
  "devDependencies": {
    "axios": "^0.22.0",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.4",
    "lighthouse": "^9.6.7",
    "puppeteer": "^10.4.0",
    "serve": "^14.0.1",
    "webpack": "^5.55.1"
  },
  "dependencies": {
    "workbox-build": "^6.3.0"
  },
  "peerDependencies": {
    "gridsome": "^0.7.23",
    "webpack": "^5.55.1"
  }
}
